<%= form_with url: "/breweries/#{@brewery.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :year_established %>
  <%= form.text_field :year_established %>
  <%= form.label :food_available %>
  <%= form.check_box :food_available %>

  <%= form.submit "Submit changes" %>
<% end %>
